{
  "name": "@topeysoft/tsccli",
  "version": "1.0.12",
  "description": "Command line interface for starting and bootstraping node js application (and unit tests) with minimal effort. ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm version patch --force &&  npm run git-publish && npm publish --access=public",
    "git-add": "git add -A .",
    "git-commit": "git commit -a -m \"code update\"",
    "git-push": "git push origin master --force",
    "git-publish": "npm run git-commit && npm run git-push"
  },
  "bin": {
    "tli": "bin/tsccli.js",
    "tcli": "bin/tsccli.js",
    "tscli": "bin/tsccli.js",
    "tsccli": "bin/tsccli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/topeysoft/tsccli.git"
  },
  "keywords": [
    "cli",
    "node",
    "typescript",
    "unit-test"
  ],
  "author": "Temitope Adeyeri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/topeysoft/tsccli/issues"
  },
  "homepage": "https://github.com/topeysoft/tsccli#readme",
  "dependencies": {
    "@types/username": "^1.0.28",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "shelljs": "^0.7.7",
    "username": "^2.3.0"
  }
}
